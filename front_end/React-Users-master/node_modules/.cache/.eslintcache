n        for (let i = 1; i <= totalPages; i++) {\n            pages.push(\n                <Pagination.Item\n                    key={i}\n                    active={i === currentPage}\n                    onClick={() => onPageChange(i)}\n                >\n                    {i}\n                </Pagination.Item>,\n            );\n        }\n\n        return pages;\n    }, [totalPages, currentPage]);\n\n    if (totalPages === 0) return null;\n    return (\n        <Pagination>\n            <Pagination.Prev\n                onClick={() => onPageChange(currentPage - 1)}\n                disabled={currentPage === 1}\n            />\n            {paginationItems}\n            <Pagination.Next\n                onClick={() => onPageChange(currentPage + 1)}\n                disabled={currentPage === totalPages}\n            />\n        </Pagination>\n    );\n}\n\nexport default PaginationTable;\n","/Users/vhnam307/Desktop/Workspace/Workproject/paint/PaintManagement/front_end/React-Users-master/src/components/DataTable/HomeHeaderMaterial/index.js",[],[],"/Users/vhnam307/Desktop/Workspace/Workproject/paint/PaintManagement/front_end/React-Users-master/src/components/DataTable/Header/index.js",[],[],"/Users/vhnam307/Desktop/Workspace/Workproject/paint/PaintManagement/front_end/React-Users-master/src/data/MaterialCompanyData/index.js",[],[],{"ruleId":"249","severity":1,"message":"250","line":6,"column":24,"nodeType":"251","messageId":"252","endLine":6,"endColumn":37},{"ruleId":"249","severity":1,"message":"253","line":10,"column":3,"nodeType":"251","messageId":"252","endLine":10,"endColumn":11},{"ruleId":"249","severity":1,"message":"254","line":14,"column":8,"nodeType":"251","messageId":"252","endLine":14,"endColumn":13},{"ruleId":"249","severity":1,"message":"253","line":4,"column":3,"nodeType":"251","messageId":"252","endLine":4,"endColumn":11},{"ruleId":"249","severity":1,"message":"255","line":3,"column":10,"nodeType":"251","messageId":"252","endLine":3,"endColumn":20},